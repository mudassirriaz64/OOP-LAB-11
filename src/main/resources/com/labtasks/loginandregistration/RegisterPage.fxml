<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.labtasks.loginandregistration.RegistrationController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="600.0" style="-fx-background-color: #87CEEB;">
         <children>
            <Label layoutX="40.0" layoutY="29.0" prefHeight="33.0" prefWidth="197.0" text="User Registration" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="34.0" layoutY="95.0" prefHeight="18.0" prefWidth="85.0" text="First Name" />
      <TextField fx:id="FirstNameTextField" layoutX="34.0" layoutY="113.0" prefHeight="25.0" prefWidth="303.0" promptText="Enter First Name" AnchorPane.topAnchor="113.0" />
      <Label layoutX="34.0" layoutY="139.0" prefHeight="18.0" prefWidth="85.0" text="Last Name" />
      <Label layoutX="34.0" layoutY="182.0" prefHeight="18.0" prefWidth="85.0" text="Username" />
      <TextField fx:id="LastNameTextField" layoutX="34.0" layoutY="157.0" prefHeight="25.0" prefWidth="303.0" promptText="Enter Last Name" />
