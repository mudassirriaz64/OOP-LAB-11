      <Button layoutX="34.0" layoutY="327.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="153.0" style="-fx-background-color: #87CEEB;" text="Submit" textFill="WHITE" onAction="#SubmitButtonAction" />
      <Button layoutX="200.0" layoutY="327.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="136.0" style="-fx-background-color: #87CEEB;" text="Clear" textFill="WHITE" onAction="#clearButtonAction" />
